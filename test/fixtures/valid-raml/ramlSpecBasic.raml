#%RAML 1.0
title: Simple raml API
mediaType: application/json
description: This is a simple raml API.
version: v3
baseUri: https://api.BasicRamlAPI.com/{version}
securitySchemes:
  basic:
    type: Basic Authentication
    description: This is basic security scheme
securedBy: basic
/users:
  /search:
    get:
      responses:
          201:
            body:
              application/json:
                properties:
                  groupName:
                    default: groupName example
                  deptCode:
                    type: number
                    default: 12345
            headers:
              Location:
                example: /invoices/45612
              Header:
                example: Bangalore
    post:
      body:
        properties:
          firstname: string
          lastname: string
          age: number
        example:
          firstname: someName
          lastname: someLastName
          age: 10
      headers:
        myHeader:
          example: headerExample
        SomeOtherHeader:
          example: OtherExample
